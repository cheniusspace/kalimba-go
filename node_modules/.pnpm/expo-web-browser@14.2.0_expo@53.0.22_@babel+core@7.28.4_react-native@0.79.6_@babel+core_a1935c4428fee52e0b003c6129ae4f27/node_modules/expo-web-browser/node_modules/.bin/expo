#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tranmuse/Development/kalimba-go/node_modules/.pnpm/expo@53.0.22_@babel+core@7.28.4_react-native@0.79.6_@babel+core@7.28.4_@types+react@19.0.14_react@19.0.0__react@19.0.0/node_modules/expo/bin/node_modules:/Users/tranmuse/Development/kalimba-go/node_modules/.pnpm/expo@53.0.22_@babel+core@7.28.4_react-native@0.79.6_@babel+core@7.28.4_@types+react@19.0.14_react@19.0.0__react@19.0.0/node_modules/expo/node_modules:/Users/tranmuse/Development/kalimba-go/node_modules/.pnpm/expo@53.0.22_@babel+core@7.28.4_react-native@0.79.6_@babel+core@7.28.4_@types+react@19.0.14_react@19.0.0__react@19.0.0/node_modules:/Users/tranmuse/Development/kalimba-go/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tranmuse/Development/kalimba-go/node_modules/.pnpm/expo@53.0.22_@babel+core@7.28.4_react-native@0.79.6_@babel+core@7.28.4_@types+react@19.0.14_react@19.0.0__react@19.0.0/node_modules/expo/bin/node_modules:/Users/tranmuse/Development/kalimba-go/node_modules/.pnpm/expo@53.0.22_@babel+core@7.28.4_react-native@0.79.6_@babel+core@7.28.4_@types+react@19.0.14_react@19.0.0__react@19.0.0/node_modules/expo/node_modules:/Users/tranmuse/Development/kalimba-go/node_modules/.pnpm/expo@53.0.22_@babel+core@7.28.4_react-native@0.79.6_@babel+core@7.28.4_@types+react@19.0.14_react@19.0.0__react@19.0.0/node_modules:/Users/tranmuse/Development/kalimba-go/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../expo/bin/cli" "$@"
else
  exec node  "$basedir/../../../expo/bin/cli" "$@"
fi
